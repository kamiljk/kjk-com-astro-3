{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "docs",
      "path": "[[workspace]]/src/content/docs"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "'blog'",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "scale",
          "name": "scale",
          "type": "string"
        },
        {
          "title": "maturity",
          "name": "maturity",
          "type": "string"
        },
        {
          "title": "source",
          "name": "source",
          "type": "string"
        },
        {
          "title": "created",
          "name": "created",
          "type": "datetime"
        },
        {
          "title": "last_tended",
          "name": "last_tended",
          "type": "datetime"
        },
        {
          "title": "epistemic_status",
          "name": "epistemic_status",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.git.enabled": true
}